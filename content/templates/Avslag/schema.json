 {
     "definitions": {},
     "$schema": "http://json-schema.org/draft-07/schema#",
     "$id": "http://example.com/root.json",
     "type": "object",
     "title": "The Root Schema",
     "required": [
         "begrunnelse",
         "paragraf",
         "enhet",
         "saksbehandler"
       ],
     "properties": {
         "begrunnelse": {
             "$id": "#/properties/begrunnelse",
             "type": "string",
             "title": "The Begrunnelse Schema",
             "default": "",
             "examples": [
                 "du ikke har krav p√• det"
               ],
             "pattern": "^(.*)$"
           },
         "paragraf": {
             "$id": "#/properties/paragraf",
             "type": "string",
             "title": "The Paragraf Schema",
             "default": "",
             "examples": [
                 "1, 2, 3"
               ],
             "pattern": "^(.*)$"
           },
         "enhet": {
             "$id": "#/properties/enhet",
             "type": "string",
             "title": "The Enhet Schema",
             "default": "",
             "examples": [
                 "Sagene"
               ],
             "pattern": "^(.*)$"
           },
         "saksbehandler": {
             "$id": "#/properties/saksbehandler",
             "type": "string",
             "title": "The Saksbehandler Schema",
             "default": "",
             "examples": [
                 "Petra Petterson"
               ],
             "pattern": "^(.*)$"
           }
       }
   }