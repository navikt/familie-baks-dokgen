{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "belop",
    "antallBarn",
    "startDato",
    "vilkarDato",
    "fodselsdato",
    "flereBarn",
    "etterbetaling",
    "etterbetalingBelop",
    "hjemmel"
  ],
  "properties": {
    "belop": {
      "$id": "#/properties/belop",
      "type": "number",
      "title": "The Belop Schema",
      "default": "",
      "examples": [
        "1054"
      ]
    },
    "antallBarn": {
      "$id": "#/properties/antallBarn",
      "type": "number",
      "title": "The antallbarn Schema",
      "default": "",
      "examples": [
        "2"
      ]
    },
    "startDato": {
      "$id": "#/properties/startDato",
      "type": "string",
      "title": "The StartDato Schema",
      "default": "",
      "examples": [
        "februar 2020"
      ],
      "pattern": "^[a-z]{3,9} \\d{4}$"
    },
    "vilkarDato": {
      "$id": "#/properties/vilkårDato",
      "type": "string",
      "title": "The Vilkårdato Schema",
      "default": "",
      "examples": [
        "dd.MM.yy"
      ],
      "pattern": "^([0]?[1-9]|[1|2][0-9]|[3][0|1])[.]([0]?[1-9]|[1][0-2])[.]([0-9]{2})$"
    },
    "fodselsdato": {
      "$id": "#/properties/fodselsdato",
      "type": "string",
      "title": "The Fodselsdato Schema",
      "default": "",
      "examples": [
        "dd.MM.yy"
      ],
      "pattern": "^([0]?[1-9]|[1|2][0-9]|[3][0|1])[.]([0]?[1-9]|[1][0-2])[.]([0-9]{2})$",
    },
    "flereBarn": {
      "$id": "#/properties/flerebarn",
      "type": "boolean",
      "title": "The Flerebarn Schema",
      "default": false,
      "examples": [
        true
      ]
  },
    "etterbetaling": {
      "$id": "#/properties/etterbetaling",
      "type": "boolean",
      "title": "The Etterbetaling Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "etterbetalingBelop": {
      "$id": "#/properties/etterbetalingsBelop",
      "type": "number",
      "title": "The Etterbetalings beløp Schema",
      "default": "",
      "examples": [
        ""
      ]
    },
    "hjemmel": {
      "$id": "#/properties/hjemmel",
      "type": "number",
      "title": "The hjemmel Schema",
      "default": "",
      "examples": [
        "§2, 4 og 11"
      ]
    },
  "fritekst": {
    "$id": "#/properties/fritekst",
    "type": "string",
    "title": "The fritekst Schema",
    "default": "fritekst",
    "examples": [
      "fritekst"
    ]
  }
}
}

