{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "belop",
    "antallBarn",
    "virkningsdato",
    "vilkårsdato",
    "barnasFodselsdatoer",
    "flereBarn",
    "hjemmel"
  ],
  "properties": {
    "belop": {
      "$id": "#/properties/belop",
      "type": "number",
      "title": "The Belop Schema",
      "default": "",
      "examples": [
        "1054"
      ]
    },
    "antallBarn": {
      "$id": "#/properties/antallBarn",
      "type": "number",
      "title": "The antallbarn Schema",
      "default": "",
      "examples": [
        "2"
      ]
    },
    "virkningsdato": {
      "$id": "#/properties/startDato",
      "type": "string",
      "title": "The StartDato Schema",
      "default": "",
      "examples": [
        "februar 2020"
      ],
      "pattern": "^[a-z]{3,9} \\d{4}$"
    },
    "vilkårsdato": {
      "$id": "#/properties/vilkårDato",
      "type": "string",
      "title": "The Vilkårdato Schema",
      "default": "",
      "examples": [
        "dd.MM.yy"
      ],
      "pattern": "^([0]?[1-9]|[1|2][0-9]|[3][0|1])[.]([0]?[1-9]|[1][0-2])[.]([0-9]{2})$"
    },
    "barnasFodselsdatoer": {
      "$id": "#/properties/fodselsdato",
      "type": "string",
      "title": "The Fodselsdato Schema",
      "default": "",
      "examples": [
        "dd.MM.yy"
      ]
    },
    "flereBarn": {
      "$id": "#/properties/flereBarn",
      "type": "boolean",
      "title": "The flereBarn Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "hjemmel": {
      "$id": "#/properties/hjemmel",
      "type": "string",
      "title": "The hjemmel Schema",
      "default": "",
      "examples": [
        "§2, 4 og 11"
      ]
    }
}
}

