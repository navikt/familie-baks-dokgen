{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "belop",
    "virkningsdato",
    "barnasFodselsdatoer",
    "etterbetaling",
    "hjemmel"
  ],
  "properties": {
    "belop": {
      "$id": "#/properties/belop",
      "type": "number",
      "title": "The Belop Schema",
      "default": "",
      "examples": [
        ""
      ]
    },
    "virkningsdato": {
      "$id": "#/properties/virkningsdato",
      "type": "string",
      "title": "The Virkningsdato Schema",
      "default": "",
      "examples": [
        "februar 2020"
      ],
      "pattern": "^[a-z]{3,9} \\d{4}$"
    },
    "barnasFodselsdatoer": {
      "$id": "#/properties/barnasFodselsdatoer",
      "type": "string",
      "title": "The BarnasFodselsdatoer Schema",
      "default": "",
      "examples": [
        "dd.MM.yy"
      ],
      "pattern": "^([0]?[1-9]|[1|2][0-9]|[3][0|1])[.]([0]?[1-9]|[1][0-2])[.]([0-9]{2})$"
    },
    "etterbetaling": {
      "$id": "#/properties/etterbetaling",
      "type": "boolean",
      "title": "The Etterbetaling Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "etterbetalingsmaaned": {
      "$id": "#/properties/etterbetalingsmaaned",
      "type": "string",
      "title": "The Etterbetalingsmaaned Schema",
      "default": "",
      "examples": [
        "januar"
      ],
      "pattern": "^[a-z]{3,9}$"
    },
    "etterbetalingsbelop": {
      "$id": "#/properties/etterbetalingsbelop",
      "type": "number",
      "title": "The Etterbetalingsbelop Schema",
      "default": "",
      "examples": [
        ""
      ]
    },
    "fritekst": {
      "$id": "#/properties/fritekst",
      "type": "string",
      "title": "The fritekst Schema",
      "default": "fritekst",
      "examples": [
        "fritekst"
      ]
    },
  "hjemmel": {
    "$id": "#/properties/hjemmel",
    "type": "string",
    "title": "The hjemmel Schema",
    "default": "",
    "examples": [
      "ยง2, 4 og 11"
    ]
    }
  }
}
