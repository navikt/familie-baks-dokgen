{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "fodselsnummer",
    "belop",
    "startDato",
    "fodselsdato",
    "etterbetaling",
    "enhet",
    "saksbehandler"
  ],
  "properties": {
    "fodselsnummer": {
      "$id": "#/properties/fodselsnummer",
      "type": "string",
      "title": "The Fodselsnummer Schema",
      "default": "",
      "examples": [
        "12345678910"
      ],
      "pattern": "^\\d{11}$"
    },
    "belop": {
      "$id": "#/properties/belop",
      "type": "number",
      "title": "The Belop Schema",
      "default": "",
      "examples": [
        ""
      ]
    },
    "startDato": {
      "$id": "#/properties/startDato",
      "type": "string",
      "title": "The Startdato Schema",
      "default": "",
      "examples": [
        "februar 2020"
      ],
      "pattern": "^[a-z]{3,9} \\d{4}$"
    },
    "fodselsdato": {
      "$id": "#/properties/fodselsdato",
      "type": "string",
      "title": "The Fodselsdato Schema",
      "default": "",
      "examples": [
        "dd.MM.yy"
      ],
      "pattern": "^([0]?[1-9]|[1|2][0-9]|[3][0|1])[.]([0]?[1-9]|[1][0-2])[.]([0-9]{2})$"
    },
    "etterbetaling": {
      "$id": "#/properties/etterbetaling",
      "type": "boolean",
      "title": "The Etterbetaling Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "etterbetalingsmaaned": {
      "$id": "#/properties/etterbetalingsmaaned",
      "type": "string",
      "title": "The Etterbetalingsmaaned Schema",
      "default": "",
      "examples": [
        "januar"
      ],
      "pattern": "^[a-z]{3,9}$"
    },
    "etterbetalingsbelop": {
      "$id": "#/properties/etterbetalingsbelop",
      "type": "number",
      "title": "The Etterbetalingsbelop Schema",
      "default": "",
      "examples": [
        ""
      ]
    },
    "enhet": {
      "$id": "#/properties/enhet",
      "type": "string",
      "title": "The Enhet Schema",
      "default": "",
      "examples": [
        ""
      ],
      "pattern": "^(.*)$"
    },
    "saksbehandler": {
      "$id": "#/properties/saksbehandler",
      "type": "string",
      "title": "The Saksbehandler Schema",
      "default": "",
      "examples": [
        ""
      ],
      "pattern": "^(.*)$"
    }
  }
}
