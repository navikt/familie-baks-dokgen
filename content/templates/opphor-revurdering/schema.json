{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "hjemler",
    "opphor",
    "maalform"
  ],
  "properties": {
    "hjemler": {
      "$id": "#/properties/hjemmel",
      "type": "array",
      "title": "The Hjemmel Schema",
      "default": [2],
      "examples": [2,4,11]
    },
    "duFaar": {
      "type": "array",
      "title": "The duFår schema",
      "items": { "$ref": "file:content/schemas/duFårSeksjon.json" }
    },
    "maalform": {
      "type": "string",
      "title": "The maalform schema",
      "default": "NB"
    },
    "erFeilutbetaling": {
      "type": "boolean",
      "title": "The erFeilutbetaling schema",
      "default": false
    }
  }
}
