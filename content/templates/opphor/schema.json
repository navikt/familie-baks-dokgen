{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "navn",
    "fodselsnummer",
    "tdato",
    "fritekst",
    "hjemler"

  ],
  "properties": {
    "navn": {
      "$id": "#/properties/navn",
      "type": "string",
      "title": "The navn Schema",
      "default": "",
      "examples": [
        "Fornavn Etternavn"
      ]
    },
    "fodselsnummer": {
      "$id": "#/properties/fodselsnummer",
      "type": "string",
      "title": "The Fodselsnummer Schema",
      "default": "",
      "examples": [
        "12345678910"
      ]
    },
    "tdato": {
      "$id": "#/properties/soknadDato",
      "type": "string",
      "title": "The dato Schema",
      "default": "DATO",
      "examples": [
        "DD.MM.YY"
      ]
    },
      "fritekst": {
        "$id": "#/properties/fritekst",
        "type": "string",
        "title": "The fritekst Schema",
        "default": "fritekst",
        "examples": [
          "fritekst"
        ]
    },
    "hjemler": {
      "$id": "#/properties/hjemmel",
      "type": "array",
      "title": "The Hjemmel Schema",
      "default": [2],
      "examples": [2,4,11]
    }
  }
}
