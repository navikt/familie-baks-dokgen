{{#*inline "titteltekst"}}
{{#with @root.teksterUtenomSpørsmål as | titteltekster | }}
{{#with (lookup titteltekster klammeTekstId) as | tekst | }}
{{~ replace tekst "{navn}" navn.verdi ~}}
{{/with}}
{{/with }}
{{/inline}}

{{#table columns=2}}
{{#eq spørsmål.oppholderSegIInstitusjon.verdi "JA"}}
<td>
<h5>{{> titteltekst klammeTekstId="[ombarnet.institusjon]" }}</h5>
{{> soknad/oppfolging spørsmål.institusjonsnavn }}
{{> soknad/oppfolging spørsmål.institusjonsadresse }}
{{> soknad/oppfolging spørsmål.institusjonspostnummer }}
{{> soknad/oppfolging-dato spørsmål.institusjonOppholdStartdato }}
{{> soknad/oppfolging-dato spørsmål.institusjonOppholdSluttdato }}
</td>
{{/eq}}
{{#eq spørsmål.boddMindreEnn12MndINorge.verdi "JA"}}
<td>
<h5>{{> titteltekst klammeTekstId="[ombarnet.sammenhengende-opphold]" }}</h5>
{{> soknad/oppfolging-dato spørsmål.nårKomBarnTilNorgeDato }}
{{> soknad/oppfolging-ja-nei spørsmål.planleggerÅBoINorge12Mnd }}
</td>
{{/eq}}
{{#eq spørsmål.barnetrygdFraAnnetEøsland.verdi "JA"}}
<td>
<h5>{{> titteltekst klammeTekstId="[ombarnet.barnetrygd-eøs]" }}</h5>
{{> soknad/oppfolging spørsmål.barnetrygdFraEøslandHvilketLand }}
</td>
{{/eq}}
{{#eq spørsmål.oppholderSegIUtland.verdi "JA"}}
<td>
<h5>{{> titteltekst klammeTekstId="[ombarnet.oppholdutland]" }}</h5>
{{> soknad/oppfolging spørsmål.oppholdsland }}
{{> soknad/oppfolging-dato spørsmål.oppholdslandStartdato }}
{{> soknad/oppfolging-dato spørsmål.oppholdslandSluttdato }}
</td>
{{/eq}}
{{/table}}