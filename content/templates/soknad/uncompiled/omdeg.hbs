<section class="soknad-section">
    <h3>{{section-number}}. {{@root.teksterUtenomSpørsmål.[pdf.omdeg.seksjonstittel]}}</h3>
    <table>
        <tr>
            {{> soknad/felt-celle ident}}
            <td>
                <h5>{{statsborgerskap.label}}</h5>
                <p>
                    {{#each statsborgerskap.verdi }}
                        {{~ this}}{{#if @last}}{{else}}, {{/if}}
                    {{/each}}
                </p>
            </td>
        </tr>
        <tr>
            {{# with (join "felles.sivilstatus.kode" sivilstand.verdi "." prefix='[' suffix=']') as | sivilstandkode | }}
                {{> soknad/felt-celle label=sivilstand.label verdi=(lookup @root.teksterUtenomSpørsmål sivilstandkode) }}
            {{/with}}
            <td>
                <h5>{{adresse.label}}</h5>
                {{#with adresse.verdi}}
                    <p>
                        {{~join adressenavn husnummer husbokstav bruksenhetsnummer " "}}<br/>
                        {{~join postnummer poststed " "}}
                    </p>
                {{/with}}
            </td>
        </tr>
        {{#if adresse.verdi.poststed}}
            <tr>
                {{#> soknad/ja-nei spørsmål.borPåRegistrertAdresse}}{{/soknad/ja-nei}}
            </tr>
        {{/if}}
    </table>
</section>

<section class="soknad-section">
    {{#with spørsmål}}
        {{#table}}
            {{#> soknad/ja-nei værtINorgeITolvMåneder oppfølgingCondition="NEI" }}{{/soknad/ja-nei}}
            {{#> soknad/ja-nei planleggerÅBoINorgeTolvMnd oppfølgingCondition="NEI" }}{{/soknad/ja-nei}}
        {{/table}}
    {{/with}}
</section>
